
{% extends "block.html.twig" %}

{% block content %}
  <div class="block-with-1-columns__wrapper">
    <div class="block-with-1-columns__image">
        {{ content.field_image }}
    </div>
    <div class="container">
      <div class="block-with-1-columns__inner">
        {{ content.field_title }}
        <div class="field field--name-field-body field--type-entity-reference-revisions field--label-hidden field--item">
          <div class="paragraph paragraph--type--ck-editor paragraph--view-mode--default">
            <div data-quickedit-field-id="paragraph/76/field_editor/uk/default" class="field field--name-field-editor field--type-text-long field--label-hidden field--item">
              <form action="//drupal.us12.list-manage.com/subscribe/post?u=755c8ce5d227aad4cd8e371d8&amp;id=2bb0856802" class="validate" id="mc-embedded-subscribe-form" method="post" name="mc-embedded-subscribe-form" target="_blank" data-drupal-form-fields="mce-EMAIL,mc-embedded-subscribe">
                <div id="mc_embed_signup_scroll">
                  <input class="email" id="mce-EMAIL" name="EMAIL" placeholder='{{- "Enter your e-mail"|t -}}' required="true" type="email" value="">
                  <input class="button" id="mc-embedded-subscribe" name="subscribe" type="submit" value='{{- "Submit"|t -}}'>
                </div>
              </form>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

{% endblock %}