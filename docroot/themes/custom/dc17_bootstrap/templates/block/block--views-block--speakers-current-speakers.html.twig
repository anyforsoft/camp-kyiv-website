
{%
  set classes = [
    'block',
    'block-' ~ configuration.provider|clean_class,
    'block-' ~ plugin_id|clean_class,
    'clearfix',
    elements['#configuration'].bundle|clean_class,
    'current-speakers'
  ]
%}

<section{{ attributes.addClass(classes) }}>
  {{ title_prefix }}
  {% if label %}
    <h2{{ title_attributes.addClass('block-title') }}>{{ label }}</h2>
  {% endif %}
  {{ title_suffix }}

  {% block content %}
    {{ content }}
  {% endblock %}

  <div class="presentation__return">
    <a href="{{ path('entity.node.canonical', {'node': 43 }) }}" class="btn btn-default btn-inverse">{{ 'Presentations and speakers'|t }}</a>
  </div>
</section>

